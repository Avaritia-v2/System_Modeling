        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SystemModeling"
        mc:Ignorable="d"
        Title="Graph" Height="800" Width="1200">
    <Grid>
        <TextBlock Name="GraphName" HorizontalAlignment="Center" FontSize="25" TextWrapping="Wrap"></TextBlock>
